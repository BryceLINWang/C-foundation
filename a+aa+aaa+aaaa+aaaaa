#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int main()
{   
	int i, a;
	int sum1 = 0;
	int sum= 0;
	printf("输入a:\n");
	scanf("%d", &a);
	for (i = 1; i <= 5; i++)
	{
		sum1 += a;
		sum = sum + sum1;
		a = a * 10;
	}
	printf("a+aa+aaa+aaaa+aaaaa和为%d",sum);
		system("pause");
	return 0;
}
